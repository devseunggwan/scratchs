    "import json\n",
    "\n",
    "from lzstring import LZString\n",
    "\n",
    "\n",
    "json_data = json.loads(open(\"./sample.json\").read())\n",
    "decoded_json = LZString.decompressFromEncodedURIComponent(json_data[\"encData\"])\n",
    "\n",
    "with open(\"./decoded.json\", \"w\") as f:\n",
    "    f.write(decoded_json)"
   ]
  }
